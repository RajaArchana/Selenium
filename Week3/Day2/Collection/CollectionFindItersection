package assignment15;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public class CollectionFindItersection {

	public static void main(String[] args) {
		Integer[] data = { 3, 2, 11, 4, 6, 7 };
		Integer[] data1 = { 1, 2, 8, 4, 9, 7 };
		List<Integer> list = Arrays.asList(data);
		int length1 = data1.length;
		System.out.println("Intersection of the two arrays Using collection");
		for (int i = 0; i < length1; i++) {
			int result=Collections.frequency(list, data1[i]);
			if(result>=1) {
			System.out.println(data1[i]);
		}}
		
	}

}
